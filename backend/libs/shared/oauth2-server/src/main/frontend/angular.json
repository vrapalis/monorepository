{
  "version": 1,
  "projects": {
    "oauth2-feature-login": {
      "projectType": "library",
      "root": "libs/oauth2/feature/login",
      "sourceRoot": "libs/oauth2/feature/login/src",
      "prefix": "frontend",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/oauth2/feature/login"],
          "options": {
            "jestConfig": "libs/oauth2/feature/login/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/oauth2/feature/login/src/**/*.ts",
              "libs/oauth2/feature/login/src/**/*.html"
            ]
          }
        }
      },
      "tags": ["scope:oauth2","type:feature"]
    },
    "oauth2-ui": {
      "projectType": "application",
      "root": "apps/oauth2/ui",
      "sourceRoot": "apps/oauth2/ui/src",
      "prefix": "frontend",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "../resources/static/",
            "index": "apps/oauth2/ui/src/index.html",
            "main": "apps/oauth2/ui/src/main.ts",
            "polyfills": "apps/oauth2/ui/src/polyfills.ts",
            "tsConfig": "apps/oauth2/ui/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/oauth2/ui/src/favicon.ico",
              "apps/oauth2/ui/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "./node_modules/bootstrap/dist/css/bootstrap.min.css",
              "apps/oauth2/ui/src/styles.scss"
            ],
            "scripts": [
              "./node_modules/@popperjs/core/dist/umd/popper.min.js",
              "./node_modules/bootstrap/dist/js/bootstrap.min.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/oauth2/ui/src/environments/environment.ts",
                  "with": "apps/oauth2/ui/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "oauth2-ui:build:production"
            },
            "development": {
              "browserTarget": "oauth2-ui:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "oauth2-ui:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/oauth2/ui/src/**/*.ts",
              "apps/oauth2/ui/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/oauth2/ui"],
          "options": {
            "jestConfig": "apps/oauth2/ui/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": ["scope:oauth2", "type:app"]
    },
    "oauth2-ui-e2e": {
      "root": "apps/oauth2/ui-e2e",
      "sourceRoot": "apps/oauth2/ui-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/oauth2/ui-e2e/cypress.json",
            "devServerTarget": "oauth2-ui:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "oauth2-ui:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/oauth2/ui-e2e/**/*.{js,ts}"]
          }
        }
      },
      "tags": [],
      "implicitDependencies": ["oauth2-ui"]
    },
    "shared-model": {
      "projectType": "library",
      "root": "libs/shared/model",
      "sourceRoot": "libs/shared/model/src",
      "prefix": "frontend",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared/model"],
          "options": {
            "jestConfig": "libs/shared/model/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared/model/src/**/*.ts",
              "libs/shared/model/src/**/*.html"
            ]
          }
        }
      },
      "tags": ["scope:shared", "type:model"]
    },
    "shared-ui": {
      "projectType": "library",
      "root": "libs/shared/ui",
      "sourceRoot": "libs/shared/ui/src",
      "prefix": "frontend",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared/ui"],
          "options": {
            "jestConfig": "libs/shared/ui/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared/ui/src/**/*.ts",
              "libs/shared/ui/src/**/*.html"
            ]
          }
        }
      },
      "tags": ["scope:shared", "type:ui"]
    },
    "shared-util": {
      "projectType": "library",
      "root": "libs/shared/util",
      "sourceRoot": "libs/shared/util/src",
      "prefix": "frontend",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared/util"],
          "options": {
            "jestConfig": "libs/shared/util/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared/util/src/**/*.ts",
              "libs/shared/util/src/**/*.html"
            ]
          }
        }
      },
      "tags": ["scope:shared", "type:util"]
    }
  }
}
