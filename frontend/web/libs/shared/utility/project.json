{
  "projectType": "library",
  "root": "libs/shared/utility",
  "sourceRoot": "libs/shared/utility/src",
  "prefix": "web",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/shared/utility"],
      "options": {
        "jestConfig": "libs/shared/utility/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/shared/utility/src/**/*.ts",
          "libs/shared/utility/src/**/*.html"
        ]
      }
    }
  },
  "tags": ["scope:shared", "type:utility"]
}
